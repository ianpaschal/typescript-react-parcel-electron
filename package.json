{
  "name": "typescript-react-parcel-electron",
  "productName": "Ian's Boilerplate",
  "version": "1.0.0",
  "description": "A Typescript + React + Parcel + Electron Boilerplate",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "tsc ./src/main.ts --outDir ./dist",
    "build-renderer": "parcel build ./src/renderer.html --public-url ./ --out-dir ./dist",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianpaschal/typescript-react-parcel-electron.git"
  },
  "author": {
    "name": "Ian Paschal",
    "url": "https://github.com/ianpaschal"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianpaschal/typescript-react-parcel-electron/issues"
  },
  "keywords": [
    "boilerplate",
    "electron",
    "parcel",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/ianpaschal/typescript-react-parcel-electron#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/node": "^12.0.8",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.8.4",
    "electron": "^5.0.3",
    "eslint": "^5.16.0",
    "parcel-bundler": "^1.12.3",
    "typescript": "^3.5.1",
    "typescript-eslint-parser": "^22.0.0"
  }
}
